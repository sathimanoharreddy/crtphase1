class shopping:
    def dresstype(self):
        print("type of the dress")
    def price(self):
        print("price of the dress the user wants")
    def size(self):
        print("the size of the user")
    def display(self):
        print("all the details of the user")
chandu=shopping()
chandu.dresstype()
chandu.price()
chandu.size()
chandu.display()

